{
	"compilerOptions": {
		"declaration": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"lib": [
			"dom",
			"es5",
			"es2015.iterable",
			"es2015.promise",
			"es2015.symbol",
			"es2015.symbol.wellknown"
		],
		"module": "umd",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"outDir": "_build/",
		"removeComments": false,
		"sourceMap": true,
		"strict": true,
		"target": "es5",
		"types": [ "intern" ]
	}
}

